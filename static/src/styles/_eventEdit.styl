.editInput{
	width : 450px;
	height : 35px;
	display:block;
	margin-left : 0px;
	margin-top : 25px;
	font-size:18px;
}

.editDiv{
	margin : 50px 0 0 100px;
	width : 800px;
	height: 400px;
}

.editDiscard{
	font-size : 15px;
	margin-left : 50px;
	padding: 5px 10px;
	background: #4479BA;
	color: #FFF;
	-webkit-border-radius: 4px;
	-moz-border-radius: 4px;
	border-radius: 4px; 
	border:none;
	-webkit-transition-duration: 0.2s;
	-moz-transition-duration: 0.2s;
	transition-duration: 0.2s;
	-webkit-user-select:none;
	-moz-user-select:none;
	-ms-user-select:none;
	user-select:none;
}

.editSave{
	font-size:15px;
	padding: 5px 10px;
	background:#B22222;
	color : #FFF;
	-webkit-border-radius: 4px;
	-moz-border-radius: 4px;
	border-radius: 4px; 
	border:none;
	-webkit-transition-duration: 0.2s;
	-moz-transition-duration: 0.2s;
	transition-duration: 0.2s;
	-webkit-user-select:none;
	-moz-user-select:none;
	-ms-user-select:none;
	user-select:none;
}

.editDate{
	margin-left : 0px;
	margin-top : 25px;
}

.labelStart{
	margin-left : 0px;
	margin-top : 25px;
}

.editTimePicker{
	width : 80px;
	height : 25px;
}

.start{
	margin-top:25px;
}