.weeklyViewtd{
	border : 1px solid #c0c0c0;
	width: 110px;
	height: 70px;
	max-height: 60px;
	max-width: 110px;
	text-align: left;
	overflow: hidden;
}

.weeklyViewtable{
	border-collapse: collapse;
    table-layout: fixed;
    width: 67%;
    margin: 0 auto;
}

.weeklyViewth{
	padding: 5px;
	border : 1px solid #c0c0c0;
	width : 100px;
	padding-top: 15px;
	align-content: justify;
	text-align: center;
}

.weeklyViewtbody{
	display: block;
	position: relative;
	overflow:auto;
	width : 920px;
	margin: 0 auto;
	height: 500px;
}


.weeklyViewSpan{
	height: 14px;
	padding-top: 5px;
}

.old{
	color : gray;
}